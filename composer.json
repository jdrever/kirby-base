{
  "name": "open-foundations/kirby-base",
  "description": "A set of base classes and blueprint for web development in a plugin format using Kirby CMS",
  "type": "kirby-plugin",
  "license": "MIT",
  "version": "1.2.7",
  "autoload": {
    "psr-4": {
      "BSBI\\WebBase\\": "src/"
    }
  },
  "authors": [
    {
      "name": "James Drever",
      "email": "james.drever@bsbi.org"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  },
  "require": {
    "getkirby/composer-installer": "^1.2"
  },
  "minimum-stability": "stable"
}
